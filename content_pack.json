  }, {
    "name" : "TIMESTAMP_ISO8601",
    "pattern" : "%{YEAR}-%{MONTHNUM}-%{MONTHDAY}[T ]%{HOUR}:?%{MINUTE}(?::?%{SECOND})?%{ISO8601_TIMEZONE}?"
  }, {
    "name" : "USER",
